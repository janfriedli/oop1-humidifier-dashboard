<GridPane style="-fx-background-color: #FAFAFA;" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="humidifier.Dashboard">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="315.0" minWidth="10.0" prefWidth="122.0" />
    <ColumnConstraints hgrow="SOMETIMES" maxWidth="521.0" minWidth="10.0" prefWidth="518.0" />
  </columnConstraints>
  <rowConstraints>
      <RowConstraints maxHeight="221.0" minHeight="10.0" prefHeight="54.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="422.0" minHeight="10.0" prefHeight="398.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
    <AnchorPane minHeight="0.0" minWidth="0.0" style="-fx-background-color: #FAFAFA #FAFAFA;" GridPane.rowIndex="1">
         <children>
            <JFXTextField fx:id="lower" layoutX="8.0" layoutY="31.0" prefHeight="26.0" prefWidth="99.0" promptText="Lower Bound" text="30" />
            <JFXTextField layoutX="8.0" layoutY="93.0" prefHeight="26.0" prefWidth="99.0" promptText="Upper Bound" text="40" />
            <Label layoutX="8.0" layoutY="14.0" text="Lower Bound" />
            <Label layoutX="8.0" layoutY="76.0" text="Upper Bound" />
            <JFXButton defaultButton="true" layoutX="8.0" layoutY="242.0" onAction="#updateButtonClicked" ripplerFill="#69a46b" text="Update" />
         </children>
      </AnchorPane>
      <LineChart fx:id="linechart" GridPane.columnIndex="1" GridPane.rowIndex="1">
        <xAxis>
          <CategoryAxis label="Date" prefHeight="24.0" prefWidth="421.0" side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" label="Percent" side="LEFT" tickLabelGap="10.0" tickLength="5.0" tickMarkVisible="false" />
        </yAxis>
      </LineChart>
      <AnchorPane style="-fx-background-color: #ec407a#ec407a;">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Settings">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane style="-fx-background-color: #ec407a#ec407a;" GridPane.columnIndex="1">
         <children>
            <Label layoutX="177.0" layoutY="14.0" text="Relative Humidity">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</GridPane>
